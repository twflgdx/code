int main()
{
	int H=0;
	int n=0;
	int a=0;
	int b=0;
	for(H=1;H<=4;H++)
	{
		for(a=3;a>=H;a--)
		{
			putchar(' ');
		    putchar(' ');
		}
		for(n=1;n<=(2*H-1);n++)
		{
		putchar('*');
		putchar(' ');
		}
		for(b=3;b>=H;b--)
		{
			putchar(' ');
		    putchar(' ');
		}
		putchar('\n');
	}

	return 0;
}
